{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "xv-random-quotes/specific-quote",
    "title": "Specific Quote",
    "category": "widgets",
    "icon": "format-quote",
    "description": "Display a specific quote by ID",
    "keywords": [
        "quote",
        "specific",
        "id"
    ],
    "textdomain": "stray-quotes",
    "attributes": {
        "quoteId": {
            "type": "number",
            "default": 0
        },
        "disableaspect": {
            "type": "boolean",
            "default": false
        }
    },
    "supports": {
        "html": false,
        "align": true
    },
    "editorScript": "file:./index.js"
}